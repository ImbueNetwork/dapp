    "passport-google-oidc": "^0.1.0",
    "passport-jwt": "^4.0.0",
    "passport-openidconnect": "^0.1.1",
    "pg": "^8.7.1",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuidv4": "^6.2.12",
    "@material/dialog": "^13.0.0",
    "@material/list": "^13.0.0",
